<Window x:Class="Synergy.WPF.Navigation.Windows.DialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Synergy.WPF.Navigation.Windows"
        
        x:Name="Dialog"
        
        xmlns:comps="clr-namespace:Synergy.WPF.Navigation.Components"
        xmlns:attached="clr-namespace:Synergy.WPF.Navigation.AttachedProperties"
        
        attached:DialogResultAttachedProperty.Value="{Binding ElementName=Dialog, Path=DataContext.DialogResult}"
        
        mc:Ignorable="d"
        Title="DialogWindow" Height="450" Width="800">

</Window>
